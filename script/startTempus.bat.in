'@ECHO OFF
SET BIN_DIR="${CMAKE_INSTALL_PREFIX}/bin"
SET PLUGIN_DIR="${PLUGIN_INSTALL_DIR}"

SET ARG=
:Loop
IF "%1"=="" GOTO Continue
SET ARG=%ARG% -l %1
SHIFT
GOTO Loop
:Continue
SET TEMPUS_DATA_DIRECTORY=${CMAKE_INSTALL_PREFIX}\${TEMPUS_INSTALL_DATA_DIRECTORY}\data
%BIN_DIR%\wps -c %PLUGIN_DIR% -p 9000 %ARG%

